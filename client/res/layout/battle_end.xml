<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/tutorial"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">

	<ImageView android:id="@+id/img_battle_end"
		android:layout_width="fill_parent" android:layout_height="fill_parent"
		android:layout_alignParentTop="true" android:layout_alignParentLeft="true">
	</ImageView>

	<TextView android:id="@+id/txt_battle_end_title"
		android:layout_width="250px" android:layout_height="50px"
		android:textSize="18sp" android:textStyle="bold" android:text="Battle result"
		android:textColor="#aa2222ee" android:shadowDx="1.2" android:shadowDy="1.1"
		android:layout_alignParentTop="true" android:layout_centerHorizontal="true">
	</TextView>

	<TextView android:id="@+id/txt_battle_end"
		android:layout_width="305px" android:layout_height="347px"
		android:text="TextView" android:textSize="18sp" android:typeface="monospace"
		android:textStyle="italic" android:layout_alignParentBottom="true"
		android:layout_centerHorizontal="true">
	</TextView>


	<Button android:id="@+id/btn_battle_end_ok"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:typeface="sans" android:textStyle="bold"
		android:gravity="center" android:layout_alignParentBottom="true"
		android:layout_centerHorizontal="true" android:text="OK">
	</Button>
</RelativeLayout>
